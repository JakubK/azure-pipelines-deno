    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Download Deno",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/DenoDownload/index.js",
        "env": {
          "AGENT_TEMPDIRECTORY": "${workspaceFolder}/DenoDownload/temp",
          "AGENT_TOOLSDIRECTORY": "${workspaceFolder}/DenoDownload/temp"
        },
        "outputCapture": "std"
      }
    ]
}
